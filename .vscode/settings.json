{
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "[javascript]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": true,
    "deno.import_map": "./import_map.json",
    "deno.import_intellisense_origins": {
        "https://deno.land": true,
    },
}